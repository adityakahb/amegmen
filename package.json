{
  "name": "amegmen",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "generatedata": "node ./scripts/generatedata.js",
    "compile:sass": "node-sass --source-map true src/amegmen/sass/ -o dist/amegmen/styles",
    "compile:ts": "tsc",
    "uglify:js": "uglifyjs dist/amegmen/scripts/amegmen.js -o dist/amegmen/scripts/amegmen.min.js -c -m",
    "uglify:css": "uglifycss dist/amegmen/styles/amegmen.css --output dist/amegmen/styles/amegmen.min.css",
    "build:dev": "npm run compile:ts && npm run compile:sass",
    "build:prod": "npm run build:dev && npm run uglify:js && npm run uglify:css",
    "compile:sitesass": "node-sass --source-map true src/site/sass/ -o dist/site/styles && uglifycss dist/site/styles/index.css --output dist/site/styles/index.min.css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adityakahb/amegmen.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adityakahb/amegmen/issues"
  },
  "homepage": "https://github.com/adityakahb/amegmen#readme",
  "dependencies": {
    "bootstrap": "^4.5.2",
    "node-sass": "^4.14.1",
    "typescript": "^4.0.3",
    "uglify-js": "^3.10.4",
    "uglifycss": "0.0.29"
  }
}
