{
  "name": "amegmen",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "scripts": {
    "generatedata": "node ./scripts/generatedata.js",
    "compile:sass": "node-sass --source-map true src/sass/ -o dist/styles/",
    "compile:ts": "tsc",
    "uglify:js": "uglifyjs dist/scripts/index.js -o dist/scripts/index.min.js -c -m",
    "uglify:css": "uglifycss dist/styles/index.css --output dist/styles/index.min.css",
    "build:dev": "npm run compile:ts && npm run compile:sass",
    "build:prod": "npm run build:dev && npm run uglify:js && npm run uglify:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adityakahb/amegmen.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/adityakahb/amegmen/issues"
  },
  "homepage": "https://github.com/adityakahb/amegmen#readme",
  "dependencies": {
    "node-sass": "^4.14.1",
    "tslib": "^2.0.1",
    "typescript": "^4.0.2",
    "uglify-js": "^3.10.4",
    "uglifycss": "0.0.29"
  }
}
