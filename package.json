  "scripts": {
    "clean": "rimraf dist",
    "build:js": "rollup -c --config-file rollup.config.js --environment NODE_ENV:development",
    "build:css": "rollup -c --config-file rollup.config.js --environment NODE_ENV:development",
    "build": "npm-run-all clean build:js build:css",
    "build:prod": "cross-env NODE_ENV=production npm-run-all clean build:js build:css",
    "serve": "browser-sync start --server public --files 'public/**/*', 'dist/**/*', 'src/**/*'",
